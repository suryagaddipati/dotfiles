    "on-click": "/home/surya/code/dotfiles/bash_functions/pomodoro-tracker.sh toggle; /home/surya/code/dotfiles/bash_functions/pomodoro-tracker.sh check-completion; pkill -SIGRTMIN+8 waybar",
    "on-click-middle": "/home/surya/code/dotfiles/bash_functions/pomodoro-tracker.sh start --add 25m --notify; /home/surya/code/dotfiles/bash_functions/pomodoro-tracker.sh check-completion; pkill -SIGRTMIN+8 waybar",
    "on-click-right": "/home/surya/code/dotfiles/bash_functions/pomodoro-tracker.sh stop; pkill -SIGRTMIN+8 waybar",
    "tooltip": true,
    "tooltip-format": "Left: Toggle (pause/resume)\nMiddle: Start new 25min session\nRight: Stop timer",
    "signal": 8,
    "interval": 1
  },
  "network": {
    "format-icons": ["Û∞§Ø", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"],
